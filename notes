database stuff:
table "channels":
    channel_id integer pkey
    name text
table "active_users":
    user_id interger pkey
    name text
table "messages":
    message_id integer pkey
    channel_id integer (channels.channel_id fkey)
    user_id integer (active_users.user_id fkey)
    message text